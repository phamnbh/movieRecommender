<h1>{{title}}</h1>
<nav class="navbar navbar-light bg-faded">
	{{#each genres}}
		<a class="navbar-brand" href="/top/{{this}}">{{this}}</a>
	{{/each}}
</nav>


<div class="outside">
	{{#each topMovies}}
		<div class="col-md-3">
			<div class="well text-center">
				<img src="{{this.[1]}}" class="movie">
				<h5>{{this.[0]}}</h5>
				<!-- <a onclick="movieSelected('${movie.imdbID}')" class="btn btn-primary" href="#">Movie Details</a> -->
			</div>
		</div>
	    <!-- <img src="http://img.omdbapi.com/?apikey=96b07974&i=tt{{this}}">  -->
	{{/each}} 
</div>

